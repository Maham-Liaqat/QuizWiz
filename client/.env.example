# Copy this file to .env (local), .env.development, or .env.production
# Vite env vars must be prefixed with VITE_

# Base URL for your backend API
VITE_API_BASE_URL=http://localhost:4000

# Toggle auth flows on the client if backend auth is enabled
VITE_AUTH_ENABLED=true

# Environment name for display/diagnostics
VITE_ENV=development
